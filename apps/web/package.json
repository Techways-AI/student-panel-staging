{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next dev",
    "dev:fast": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-warnings\" NODE_ENV=development next dev",
    "dev:ultra": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --no-warnings\" NODE_ENV=development next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:css-fresh": "rm -rf .next .swc && npm run dev:fast",
    "build:css-fresh": "rm -rf .next .swc && npm run build",
    "css:cache-bust": "node add-css-cache-bust.js",
    "dev:css-cache-bust": "npm run css:cache-bust && npm run dev:css-fresh",
    "build:css-cache-bust": "npm run css:cache-bust && npm run build:css-fresh",
    "generate:pwa-assets": "node generate-pwa-assets.js"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@vimeo/player": "^2.29.3",
    "axios": "^1.9.0",
    "buffer": "^6.0.3",
    "critters": "^0.0.23",
    "katex": "^0.16.22",
    "lucide-react": "^0.542.0",
    "mammoth": "^1.9.1",
    "mermaid": "^10.9.1",
    "next": "^14.2.5",
    "posthog-js": "^1.278.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-player": "^2.16.0",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "rehype-mathjax": "^7.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwindcss": "^4.1.7",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "autoprefixer": "^10.4.21",
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^10.0.0",
    "cssnano": "^7.1.1",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.1.1",
    "pwa-asset-generator": "^8.1.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}

